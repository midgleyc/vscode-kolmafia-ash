{
	"name": "vscode-kolmafia-ash",
	"displayName": "KoLmafia ASH++",
	"description": "Language support for KoLmafia ASH",
	"version": "0.5.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"publisher": "danielyxie",
	"icon": "images/swordbot.png",
	"galleryBanner": {
		"color": "#EEEEEE",
		"theme": "light"
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/danielyxie/vscode-kolmafia-ash.git"
	},
	"homepage": "https://github.com/danielyxie/vscode-kolmafia-ash#readme",
	"bugs": {
		"url": "https://github.com/danielyxie/vscode-kolmafia-ash/issues"
	},
	"license": "BSD-2-Clause",
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:ash"
	],
	"keywords": [
		"ash",
		"kol",
		"kolmafia",
		"mafia"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "ash",
				"aliases": [
					"KoLMafia ASH",
					"ash"
				],
				"extensions": [
					".ash"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ash",
				"scopeName": "source.ash",
				"path": "./dist/ash.json"
			}
		]
	},
	"scripts": {
		"generate-syntax": "js-yaml syntaxes/ash.yml > syntaxes/ash.tmLanguage.json",
		"generate-parser": "antlr4ts -visitor -o src/ashparser src/ashparser/AshLexer.g4 src/ashparser/AshParser.g4",
		"vscode:prepublish": "npm run compile",
		"compile": "npm run generate-syntax && npm run generate-parser && webpack --mode production",
		"compile-dev": "npm run generate-syntax && npm run generate-parser && webpack --mode development",
		"watch": "node watch-yml & tsc -watch -p ./",
		"webpack-dev": "webpack --mode development --watch",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"antlr4ts-cli": "^0.5.0-alpha.3",
		"glob": "^7.1.5",
		"js-yaml": "^3.14.0",
		"mocha": "^6.2.2",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10"
	},
	"dependencies": {
		"antlr4ts": "^0.5.0-alpha.3"
	}
}
